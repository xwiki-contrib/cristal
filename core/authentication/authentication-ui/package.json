  "license": "LGPL 2.1",
  "author": "XWiki Org Community <contact@xwiki.org>",
  "homepage": "https://cristal.xwiki.org/",
  "repository": {
    "type": "git",
    "directory": "/core/authentication/authentication-ui",
    "url": "https://github.com/xwiki-contrib/cristal/"
  },
