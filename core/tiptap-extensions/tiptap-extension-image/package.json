{
  "name": "@xwiki/cristal-tiptap-extension-image",
  "version": "0.22.0",
  "license": "LGPL 2.1",
  "author": "XWiki Org Community <contact@xwiki.org>",
  "homepage": "https://cristal.xwiki.org/",
  "repository": {
    "type": "git",
    "directory": "/core/tiptap-extension/tiptap-extension-image",
    "url": "git+https://github.com/xwiki-contrib/cristal.git"
  },
  "bugs": {
    "url": "https://jira.xwiki.org/projects/CRISTAL/"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "api-extractor:local": "api-extractor run --local",
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\" --max-warnings=0",
    "test": "vitest --run",
    "typecheck": "vue-tsc"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@tiptap/extension-image": "catalog:",
    "@tiptap/pm": "catalog:",
    "@tiptap/vue-3": "catalog:",
    "@xwiki/cristal-api": "workspace:*",
    "@xwiki/cristal-attachments-api": "workspace:*",
    "@xwiki/cristal-document-api": "workspace:*",
    "@xwiki/cristal-link-suggest-api": "workspace:*",
    "@xwiki/cristal-model-api": "workspace:*",
    "@xwiki/cristal-model-reference-api": "workspace:*",
    "@xwiki/cristal-model-remote-url-api": "workspace:*",
    "@xwiki/cristal-tiptap-link-suggest-ui": "workspace:*",
    "lodash-es": "catalog:",
    "prosemirror-markdown": "catalog:",
    "vue-i18n": "catalog:",
    "vue-tippy": "catalog:",
    "vue3-draggable-resizable": "catalog:"
  },
  "peerDependencies": {
    "@tiptap/core": "2.13.0",
    "reflect-metadata": "0.x",
    "vue": "3.x"
  },
  "devDependencies": {
    "@tiptap/core": "catalog:",
    "@types/lodash-es": "catalog:",
    "@xwiki/cristal-dev-config": "workspace:*",
    "@xwiki/cristal-dsapi": "workspace:*",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.es.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.umd.js"
        }
      }
    },
    "main": "./dist/index.es.js",
    "types": "./dist/index.d.ts"
  }
}
