{
  "name": "@xwiki/cristal-editors-blocknote",
  "version": "0.25.0",
  "license": "LGPL 2.1",
  "author": "XWiki Org Community <contact@xwiki.org>",
  "homepage": "https://cristal.xwiki.org/",
  "repository": {
    "type": "git",
    "directory": "editors/blocknote",
    "url": "git+https://github.com/xwiki-contrib/cristal.git"
  },
  "bugs": {
    "url": "https://jira.xwiki.org/projects/CRISTAL/"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "api-extractor:local": "api-extractor run --local",
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\" --max-warnings=0",
    "test": "vitest --run",
    "typecheck": "vue-tsc"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@xwiki/cristal-alerts-api": "workspace:*",
    "@xwiki/platform-api": "catalog:",
    "@xwiki/platform-backend-api": "catalog:",
    "@xwiki/platform-collaboration-api": "catalog:",
    "@xwiki/platform-document-api": "catalog:",
    "@xwiki/platform-dsapi": "catalog:",
    "@xwiki/platform-editors-blocknote-headless": "catalog:",
    "@xwiki/platform-icons": "catalog:",
    "@xwiki/platform-macros-service": "catalog:",
    "@xwiki/platform-model-api": "catalog:",
    "@xwiki/platform-model-reference-api": "catalog:",
    "@xwiki/cristal-skin": "workspace:*",
    "@xwiki/platform-uniast-api": "catalog:",
    "@xwiki/platform-uniast-markdown": "catalog:",
    "lodash-es": "catalog:",
    "vue-i18n": "catalog:"
  },
  "peerDependencies": {
    "inversify": "7.x",
    "reflect-metadata": "0.x",
    "vue": "3.x",
    "vue-router": "4.x"
  },
  "devDependencies": {
    "@types/lodash-es": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@xwiki/cristal-dev-config": "workspace:*",
    "inversify": "catalog:",
    "reflect-metadata": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.es.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.umd.js"
        }
      }
    },
    "main": "./dist/index.es.js",
    "types": "./dist/index.d.ts"
  }
}
