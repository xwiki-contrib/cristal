{
  "name": "@xwiki/cristal-skin",
  "version": "0.25.0",
  "license": "LGPL 2.1",
  "author": "XWiki Org Community <contact@xwiki.org>",
  "homepage": "https://cristal.xwiki.org/",
  "repository": {
    "type": "git",
    "directory": "skin",
    "url": "git+https://github.com/xwiki-contrib/cristal.git"
  },
  "bugs": {
    "url": "https://jira.xwiki.org/projects/CRISTAL/"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "scripts": {
    "api-extractor:local": "api-extractor run --local",
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\" --max-warnings=0",
    "test": "vitest --run",
    "typecheck": "vue-tsc"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@xwiki/cristal-alerts-api": "workspace:*",
    "@xwiki/cristal-alerts-ui": "workspace:*",
    "@xwiki/platform-api": "catalog:",
    "@xwiki/platform-backend-api": "catalog:",
    "@xwiki/cristal-date-ui": "workspace:*",
    "@xwiki/platform-document-api": "catalog:",
    "@xwiki/platform-dsapi": "catalog:",
    "@xwiki/cristal-extra-tabs-api": "workspace:*",
    "@xwiki/cristal-extra-tabs-ui": "workspace:*",
    "@xwiki/platform-fn-utils": "catalog:",
    "@xwiki/cristal-hierarchy-api": "workspace:*",
    "@xwiki/platform-icons": "catalog:",
    "@xwiki/cristal-info-actions-api": "workspace:*",
    "@xwiki/cristal-info-actions-ui": "workspace:*",
    "@xwiki/platform-model-api": "catalog:",
    "@xwiki/cristal-model-click-listener": "workspace:*",
    "@xwiki/platform-model-reference-api": "catalog:",
    "@xwiki/platform-navigation-tree-api": "catalog:",
    "@xwiki/cristal-page-actions-ui": "workspace:*",
    "@xwiki/cristal-settings-api": "workspace:*",
    "@xwiki/cristal-settings-configurations": "workspace:*",
    "@xwiki/cristal-uiextension-api": "workspace:*",
    "@xwiki/cristal-uiextension-ui": "workspace:*",
    "@xwiki/cristal-uniast-html": "workspace:*",
    "@xwiki/platform-uniast-markdown": "catalog:",
    "@xwiki/cristal-user-ui": "workspace:*",
    "@xwiki/cristal-wiki-config-api": "workspace:*",
    "reflect-metadata": "catalog:",
    "vue-i18n": "catalog:"
  },
  "peerDependencies": {
    "inversify": "7.x",
    "reflect-metadata": "0.x",
    "vue": "3.x",
    "vue-router": "4.x"
  },
  "devDependencies": {
    "@vue/test-utils": "catalog:",
    "@xwiki/cristal-dev-config": "workspace:*",
    "@xwiki/cristal-dev-test-utils": "workspace:*",
    "inversify": "catalog:",
    "ts-essentials": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vitest-mock-extended": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.es.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.umd.js"
        }
      }
    },
    "main": "./dist/index.es.js",
    "types": "./dist/index.d.ts"
  }
}
